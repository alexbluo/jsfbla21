{
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": "off",
    "camelcase": "off",
    "import/no-extraneous-dependencies": "off",
    "no-param-reassign": "off",
    "arrow-body-style": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js"]
      }
    }
  },
  "env": {
    "node": true
  }
}
